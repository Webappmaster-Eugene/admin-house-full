{
  "name": "Admin House - frontend",
  "author": "LUCHI",
  "version": "1.0.5",
  "description": "Admin House - frontend",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development -- next dev -p 8088",
    "start": "dotenv -e .env.production -- next start -p 8080",
    "build": "dotenv -e .env.production next build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "rm:all": "rm -rf node_modules .next out dist build",
    "re:start": "yarn rm:all && yarn install && yarn dev",
    "re:build": "yarn rm:all && yarn install && yarn build",
    "re:build-npm": "npm run rm:all && npm install && npm run build",
    "dev:ts": "yarn dev & yarn ts:watch",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.3.2",
    "@iconify/react": "^4.1.1",
    "@mui/icons-material": "^5.16.1",
    "@mui/lab": "^5.0.0-alpha.155",
    "@mui/material": "^5.15.20",
    "@mui/x-data-grid": "^7.7.0",
    "@numart/house-admin-contracts": "^2.1.3",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.6.2",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "dotenv-cli": "^7.4.2",
    "framer-motion": "^10.16.16",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "next": "^14.2.4",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "simplebar-react": "^3.2.4",
    "skott": "^0.35.0",
    "stylis": "^4.3.0",
    "stylis-plugin-rtl": "^2.1.1",
    "tailwind-merge": "^2.3.0",
    "yup": "^1.3.2",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@import-meta-env/unplugin": "^0.5.1",
    "@next/eslint-plugin-next": "^14.2.3",
    "@svgr/webpack": "^8.1.0",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.4",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.43",
    "@types/stylis": "^4.2.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.3.3"
  }
